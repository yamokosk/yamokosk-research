				<static type="twist_angle" value="0" />
				<static type="joint_offset" value="0" />
				<dynamic type="joint_angle" name="q3" />
				<static type="joint_angle_offset" value="pi/2" />
			</transform>
			<geometry type="ccylinder" name="ccylinder01">
				<parameter type="length" value="223" />
				<parameter type="radius" value="59" />
