<!--
-- Standard DH notation --
A	= [0,		450,		0,		0,		0,		0	];	% link length
alpha	= [-pi/2,	0,		pi/2,		-pi/2,	pi/2,		0	];	% link twist angle
D	= [317,	0,		0,		480,		0,		70	];	% link offset distance
theta	= [0,		0,		0,		0,		0,		0	];	% link rotation angle
offset= [0,		-pi/2,	pi/2,		0,		0,		0	];	% offset for theta (for zero-pose)
rx	= [0,		-321,		0,		0,		0,		0	];	% CG
ry	= [117,	-30,		0,		112.2,	0,		0	];
rz	= [0,		0,		48.5,		0,		-42,		-48	];

-- Modified DH notation --
A	= [0,		450,		0,		0,	 	0,		0	];	% link length
alpha	= [0, 		-pi/2,	0,		pi/2,		-pi/2,	pi/2	];	% link twist angle
D	= [317,	0,		0,		480,		0,		70	];	% link offset distance
theta	= [0,		0,		0,		0,		0,		0	];	% link rotation angle
offset= [0,		-pi/2,	pi/2,		0,		0, 		0	];	% offset for theta (for zero-pose)
rx	= [0,		129,		0,		0,		0,		0	];	% CG
ry	= [0,		-30,		-48.5,	0,		42,		0	];
rz	= [-178.5,	0,		0,		-112.2,	0,		-48	];

REMEMBER
T_ij = f(a_ij,  alpha_ij, S_j, theta_j)
-->

<scene units="mm">

	<space name="PA10">
		<body name="link1" base="world">
			<transform type="dh">
				<static type="link_length" value="0" />
				<static type="twist_angle" value="0" />
				<static type="joint_offset" value="584.2+eps" />
				<dynamic type="joint_angle" name="q1" />
				<static type="joint_angle_offset" value="0" />
			</transform>
			<geometry type="ccylinder" name="ccylinder01">
				<parameter type="length" value="200" />
				<parameter type="radius" value="112" />
				<transform type="affine">
					<static type="translation" value="0 0 -178.5" />
				</transform>
			</geometry>
		</body>

		<body name="link2" base="link1">
			<transform type="dh">
				<static type="link_length" value="0" />
