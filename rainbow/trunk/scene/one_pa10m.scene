<!--
-- Standard DH notation --
A	= [0,		450,		0,		0,		0,		0	];	% link length
alpha	= [-pi/2,	0,		pi/2,		-pi/2,	pi/2,		0	];	% link twist angle
D	= [317,	0,		0,		480,		0,		70	];	% link offset distance
theta	= [0,		0,		0,		0,		0,		0	];	% link rotation angle
offset= [0,		-pi/2,	pi/2,		0,		0,		0	];	% offset for theta (for zero-pose)
rx	= [0,		-321,		0,		0,		0,		0	];	% CG
ry	= [117,	-30,		0,		112.2,	0,		0	];
rz	= [0,		0,		48.5,		0,		-42,		-48	];

-- Modified DH notation --
A	= [0,		450,		0,		0,	 	0,		0	];	% link length
alpha	= [0, 		-pi/2,	0,		pi/2,		-pi/2,	pi/2	];	% link twist angle
D	= [317,	0,		0,		480,		0,		70	];	% link offset distance
theta	= [0,		0,		0,		0,		0,		0	];	% link rotation angle
offset= [0,		-pi/2,	pi/2,		0,		0, 		0	];	% offset for theta (for zero-pose)
rx	= [0,		129,		0,		0,		0,		0	];	% CG
ry	= [0,		-30,		-48.5,	0,		42,		0	];
rz	= [-178.5,	0,		0,		-112.2,	0,		-48	];
-->

<scene units="mm">

	<space name="PA10">
		<body name="link1" base="world">
			<transform>
				<dh link_length="0" twist_angle="0" joint_offset="317" joint_angle="0" joint_angle_offset="0" />
			</transform>
			<geometry type="ccylinder" name="ccylinder01">
				<parameter type="length" value="200" />
				<parameter type="radius" value="100" />
				<transform>
					<translation value="0 0 -178.5" />
				</transform>
			</geometry>
		</body>

		<body name="link2" base="link1">
			<transform>
				<dh link_length="450" twist_angle="-pi/2" joint_offset="0" joint_angle="0" joint_angle_offset="-pi/2" />
			</transform>
			<geometry type="ccylinder" name="ccylinder01">
				<parameter type="length" value="617.5" />
				<parameter type="radius" value="100" />
				<transform>
					<translation value="129 -30 0" />
				</transform>
			</geometry>
		</body>

		<body name="link3" base="link2">
			<transform>
				<dh link_length="0" twist_angle="0" joint_offset="0" joint_angle="0" joint_angle_offset="pi/2" />
			</transform>
			<geometry type="ccylinder" name="ccylinder01">
				<parameter type="length" value="223" />
				<parameter type="radius" value="100" />
				<transform>
					<translation value="0 -48.5 0" />
				</transform>
			</geometry>
		</body>
		
		<body name="link4" base="link3">
			<transform>
				<dh link_length="0" twist_angle="pi/2" joint_offset="480" joint_angle="0" joint_angle_offset="0" />
			</transform>
			<geometry type="ccylinder" name="ccylinder01">
				<parameter type="length" value="275" />
				<parameter type="radius" value="100" />
				<transform>
					<translation value="0 0 -112.2" />
				</transform>
			</geometry>
		</body>
		
		<body name="link5" base="link4">
			<transform>
				<dh link_length="0" twist_angle="-pi/2" joint_offset="0" joint_angle="0" joint_angle_offset="0" />
			</transform>
			<geometry type="ccylinder" name="ccylinder01">
				<parameter type="length" value="200" />
				<parameter type="radius" value="100" />
				<transform>
					<translation value="0 42 0" />
				</transform>
			</geometry>
		</body>
	</space>

	<space name="Environment">
		<body name="stand" base="world">
			<transform>
				<translation value="0.0 0.0 584.2/2" />
			</transform>
			<geometry type="box" name="box01">
				<parameter type="length" value="457.2" />
				<parameter type="width" value="609.6" />
				<parameter type="height" value="584.2" />
			</geometry>
		</body>

		<body name="floor" base="world">
			<geometry type="plane" name="plane01">
				<parameter type="normal_x" value="0" />
				<parameter type="normal_y" value="0" />
				<parameter type="normal_z" value="1" />
				<parameter type="d" value="0" />
			</geometry>
		</body>
	</space>

	<space name="Obstacles">
		<body name="obs1" base="world">
			<transform>
				<translation value="0.0 200.0 600.0" />
			</transform>
			<geometry type="sphere">
				<parameter type="radius" value="275" />
			</geometry>
		</body>

		<body name="obs2" base="world">
			<transform>
