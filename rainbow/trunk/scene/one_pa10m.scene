		</body>
		
		<body name="obs3" base="world">
			<transform>
				<translation value="0.0 600.0 200.0" />
			</transform>
			<geometry type="sphere">
				<parameter type="radius" value="275" />
			</geometry>
		</body>
		
		<body name="obs4" base="world">
			<transform>
				<translation value="0.0 -600.0 -200.0" />
			</transform>
			<geometry type="sphere">
				<parameter type="radius" value="275" />
			</geometry>
		</body>
	</space>

</scene>